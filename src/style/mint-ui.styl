/deep/ .mint-tab-item {
  display flex
  flex-direction column
  align-items center
  justify-content center
}

/deep/ .mint-header {
  background #FFFFFF
  font-size: 1rem;
  color: #333333;
  text-align: center;
  border-bottom 1px solid #EEEEEE
}
/deep/ .mint-header-title {
  overflow visible
}

/deep/ .mint-cell-text {
  font-size 0.85rem
  font-weight normal
  color #333333
}

/deep/ .mint-cell-value {
  font-size 0.85rem
  color #999999
}

/deep/ .mint-field-core::-webkit-input-placeholder {
  /* WebKit browsers */
  color: #CCCCCC;
  opacity 0.7
}

/deep/ .mint-field-core:-moz-placeholder {
  /* WebKit browsers */
  color: #CCCCCC;
  opacity 0.7
}

/deep/ .mint-field-core::-moz-placeholder {
  /* WebKit browsers */
  color: #CCCCCC;
  opacity 0.7
}

/deep/ .mint-field-core::-ms-input-placeholder {
  /* WebKit browsers */
  color: #CCCCCC;
  opacity 0.7
}

/deep/ .mint-cell {
  border-top: 1px solid #EEEEEE

}

/deep/ .mint-cell:last-child {
  border-bottom: 1px solid #EEEEEE
}

/deep/ .mint-cell-swipe-button {
  display flex
  align-items center
}

/deep/ .mint-radio-input:checked + .mint-radio-core {
  background-image linear-gradient(-134deg, #0BBFD5 0%, #6DD7B2 100%)
  border-image linear-gradient(-134deg, #0BBFD5 0%, #6DD7B2 100%)
}
/deep/ .mint-navbar .mint-tab-item.is-selected {
  border 0
}
